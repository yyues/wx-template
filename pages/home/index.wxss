.user {
  display: flex;
  align-items: center;
}
.user image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.user .name {
  color: white;
}
.page-wrapper {
  background-color: #fff;
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}
.page-wrapper .today {
  flex: 1;
  padding: 20rpx 24rpx;
}
.page-wrapper .today .header {
  margin-bottom: 20rpx;
  color: #24271e;
}
.page-wrapper .today .header text {
  color: #24271e;
}
.page-wrapper .action-wrapper {
  margin: 20rpx 0;
  min-height: 360rpx;
}
.page-wrapper .action-wrapper .item {
  border-radius: 10rpx;
  padding: 16rpx;
  margin: 0 20rpx;
  display: flex;
  align-items: center;
  border-radius: 8rpx;
}
.page-wrapper .action-wrapper .item image {
  width: 44rpx;
  height: 44rpx;
}
.page-wrapper .action-wrapper .item .content {
  display: flex;
  margin-left: 20rpx;
  align-items: center;
}
.page-wrapper .action-wrapper .item .content text {
  color: #24271e;
  font-size: 32rpx;
  line-height: 48rpx;
  letter-spacing: 0.5px;
  margin-right: 20rpx;
}
.page-wrapper .action-wrapper .item .seted text {
  font-size: 28rpx;
  color: #94a3b8;
}
.page-wrapper .action-wrapper .high {
  background-color: #f5f5f5;
}
.operation-wrapper {
  padding: 20rpx 0;
  background-color: #f5f5f5;
}
.extal {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #ffffff00;
}
.extal image {
  height: 32px;
  width: 32px;
}
.extal text {
  display: block;
  font-size: 28rpx;
  line-height: 44rpx;
  margin-top: 25rpx;
  color: #475569;
}
.white-input .van-search__content {
  background-color: #fff;
}
